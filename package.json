{
  "name": "LearnFlow",
  "version": "0.0.1",
  "author": "Umair Saleem",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "node scripts/clean.mjs",
    "prebuild": "node scripts/prebuild.mjs",
    "build": "nest build",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=debuging nest start --debug --watch",
    "start:prod": "pm2-runtime start ecosystem.config.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:band": "jest --runInBand --passWithNoTests",
    "test:watchBand": "jest --watchAll --runInBand --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests",
    "test:e2e": "jest --config jest.e2e.config.js --passWithNoTests",
    "test:e2e:watch": "jest --config jest.e2e.config.js --watch --passWithNoTests",
    "test:e2e:band": "jest --config jest.e2e.config.js --runInBand --passWithNoTests",
    "test:e2e:watchBand": "jest --config jest.e2e.config.js --watchAll --runInBand --passWithNoTests",
    "test:e2e:ci": "jest --config jest.e2e.config.js --ci --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --passWithNoTests",
    "db:migrate:generate": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/migrations.ts generate",
    "db:migrate:create": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/migrations.ts create",
    "db:migrate:run:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/migrations.ts run",
    "db:migrate:run:prod": "dotenv -e .env.production -- node dist/database/scripts/migrations.js run",
    "db:migrate:revert:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/migrations.ts revert",
    "db:migrate:revert:prod": "dotenv -e .env.production -- node dist/database/scripts/migrations.js revert",
    "db:migrate:revertAll:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/migrations.ts revertAll",
    "db:migrate:revertAll:prod": "dotenv -e .env.production -- node dist/database/scripts/migrations.js revertAll",
    "db:check:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/scripts/check-connection.ts",
    "db:check:prod": "dotenv -e .env.production -- node dist/database/scripts/check-connection.js",
    "db:seed:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/database/seeders/index.ts",
    "db:seed:prod": "dotenv -e .env.production -- node dist/database/seeders/index.js",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yaml up",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yaml down",
    "docker:prod:up": "docker-compose -f docker-compose.prod.yaml up",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yaml down"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@elastic/elasticsearch": "^9.0.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bullmq": "^5.51.1",
    "callsites": "^4.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cls-hooked": "^4.2.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cron": "^4.3.0",
    "cross-var": "^1.1.0",
    "csrf-csrf": "^4.0.0",
    "dayjs": "^1.11.13",
    "dotenv-cli": "^8.0.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.0.4",
    "joi": "^17.13.3",
    "nestjs-graceful-shutdown": "^1.0.3",
    "nodemailer": "^6.10.1",
    "pg": "^8.15.6",
    "pm2": "^6.0.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "twilio": "^5.5.2",
    "typeorm": "^0.3.22",
    "useragent": "^2.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@types/cls-hooked": "^4.3.9",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.14",
    "@types/supertest": "^6.0.3",
    "@types/useragent": "^2.3.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint:fix"
    ],
    "*.{json,ts,js,html}": [
      "npm run format:fix"
    ]
  },
  "engines": {
    "node": ">=22"
  }
}
